{
  "name": "platzivideo",
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "DB_USER": "@db-user",
    "DB_PASSWORD": "@db-password",
    "DB_HOST": "@db-host",
    "DB_NAME": "@db-name"
  }
}